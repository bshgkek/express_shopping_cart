<div class="row">
{{# each products}}

	<div class="col-sm-6 col-md-4 col-lg-4">
    	<div class="thumbnail">
    	  <img src="{{ this.imagePath }}" alt="snow-leopard" class="img-responsive">
    	  <div class="caption">
    	    <h3>{{ this.title }}</h3>
    	    <p class="description">{{ this.description }}</p>
	
    	    <div class="clearfix">
    	    	<div class="price pull-left">${{ this.price }}</div>
    	    	<a href="#" class="btn btn-success pull-right" role="button">Add to Cart</a></p>
    	    </div>
    	  </div>
    	</div>
  	</div>

{{/each}}
</div>


